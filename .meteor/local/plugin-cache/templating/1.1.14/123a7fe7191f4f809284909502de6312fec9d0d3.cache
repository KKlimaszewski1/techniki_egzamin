{"head":"","body":"","js":"\nTemplate.__checkName(\"Recipe\");\nTemplate[\"Recipe\"] = new Template(\"Template.Recipe\", (function() {\n  var view = this;\n  return HTML.ARTICLE({\n    \"class\": function() {\n      return [ \"recipe \", Blaze.If(function() {\n        return Spacebars.call(view.lookup(\"inMenu\"));\n      }, function() {\n        return \"in-menu\";\n      }) ];\n    }\n  }, \"\\n    \", HTML.H3(Blaze.View(\"lookup:name\", function() {\n    return Spacebars.mustache(view.lookup(\"name\"));\n  })), \"\\n    \", Blaze.If(function() {\n    return Spacebars.call(view.lookup(\"editMode\"));\n  }, function() {\n    return [ \"\\n      \", Blaze._TemplateWith(function() {\n      return {\n        collection: Spacebars.call(\"Recipes\"),\n        id: Spacebars.call(view.lookup(\"updateRecipeId\")),\n        type: Spacebars.call(\"update\"),\n        doc: Spacebars.call(view.lookup(\".\")),\n        autosave: Spacebars.call(true)\n      };\n    }, function() {\n      return Spacebars.include(view.lookupTemplate(\"quickForm\"));\n    }), \"\\n    \" ];\n  }, function() {\n    return [ \"\\n      \", HTML.P(Blaze.View(\"lookup:desc\", function() {\n      return Spacebars.mustache(view.lookup(\"desc\"));\n    })), \"\\n      \", HTML.P(\"\\n        \", Blaze.Each(function() {\n      return Spacebars.call(view.lookup(\"ingredients\"));\n    }, function() {\n      return [ \"\\n          \", HTML.SPAN({\n        \"class\": \"ingredient\"\n      }, Blaze.View(\"lookup:name\", function() {\n        return Spacebars.mustache(view.lookup(\"name\"));\n      }), \" - \", Blaze.View(\"lookup:amount\", function() {\n        return Spacebars.mustache(view.lookup(\"amount\"));\n      })), \"\\n        \" ];\n    }), \"\\n      \"), \"\\n      \", HTML.A({\n      href: function() {\n        return [ \"/recipe/\", Spacebars.mustache(view.lookup(\"_id\")) ];\n      }\n    }, \"View Details\"), \"\\n      \", Blaze.If(function() {\n      return Spacebars.call(view.lookup(\"inMenu\"));\n    }, function() {\n      return [ \"\\n        \", HTML.BUTTON({\n        \"class\": \"btn-deny toggle-menu\"\n      }, \"Remove From Menu\"), \"\\n      \" ];\n    }, function() {\n      return [ \"\\n        \", HTML.BUTTON({\n        \"class\": \"btn-primary toggle-menu\"\n      }, \"Add To Menu\"), \"\\n      \" ];\n    }), \"\\n    \" ];\n  }), HTML.Raw('\\n    <i class=\"fa fa-pencil\"></i>\\n    <i class=\"fa fa-trash\"></i>\\n  '));\n}));\n","bodyAttrs":{}}