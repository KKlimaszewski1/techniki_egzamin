cipe', true);\n  }\n});\n","\nTemplate.__checkName(\"Menu\");\nTemplate[\"Menu\"] = new Template(\"Template.Menu\", (function() {\n  var view = this;\n  return [ HTML.Raw('<h1 class=\"page-title\">My Current Menu</h1>\\n  '), Blaze.Each(function() {\n    return Spacebars.call(view.lookup(\"recipes\"));\n  }, function() {\n    return [ \"\\n    \", Spacebars.include(view.lookupTemplate(\"MenuItem\")), \"\\n  \" ];\n  }, function() {\n    return [ \"\\n    \", HTML.H3(\"Please add \", HTML.A({\n      href: \"/recipe-book\"\n    }, \"recipes\"), \" to your menu\"), \"\\n  \" ];\n  }) ];\n}));\n\nTemplate.__checkName(\"MenuItem\");\nTemplate[\"MenuItem\"] = new Template(\"Template.MenuItem\", (function() {\n  var view = this;\n  return HTML.DIV({\n    \"class\": \"menu-item\"\n  }, \"\\n    \", HTML.H3(Blaze.View(\"lookup:name\", function() {\n    return Spacebars.mustache(view.lookup(\"name\"));\n  })), \"\\n    \", HTML.P(Blaze.View(\"lookup:desc\", function() {\n    return Spacebars.mustache(view.lookup(\"desc\"));\n  })), \"\\n  \");\n}));\n","\nTemplate.__checkName(\"ShoppingList\");\nTemplate[\"ShoppingList\"] = new Template(\"Template.ShoppingList\", (function() {\n  var view = this;\n  return [ HTML.Raw('<h1 class=\"page-title\">My Shopping List</h1>\\n  '), HTML.UL({\n    \"class\": \"shopping-list\"\n  }, \"\\n    \", Blaze.Each(function() {\n    return Spacebars.call(view.lookup(\"shoppingList\"));\n  }, function() {\n    return [ \"\\n      \", Blaze.Each(function() {\n      return Spacebars.call(view.lookup(\"ingredients\"));\n    }, function() {\n      return [ \"\\n        \", HTML.LI(Blaze.View(\"lookup:name\", function() {\n        return Spacebars.mustache(view.lookup(\"name\"));\n      }), \" - \", HTML.SPAN({\n        \"class\": \"amount\"\n      }, Blaze.View(\"lookup:amount\", function() {\n        return Spacebars.mustache(view.lookup(\"amount\"));\n      }))), \"\\n      \" ];\n    }), \"\\n    \" ];\n  }, function() {\n    return [ \"\\n      \"