      href: function() {\n        return [ \"/zadanie/\", Spacebars.mustache(view.lookup(\"_id\")) ];\n      }\n    }, \"View Details\"), \"\\n      \", Blaze.If(function() {\n      return Spacebars.call(view.lookup(\"inMenu\"));\n    }, function() {\n      return [ \"\\n        \", HTML.BUTTON({\n        \"class\": \"btn-deny toggle-menu\"\n      }, \"Remove From Menu\"), \"\\n      \" ];\n    }, function() {\n      return [ \"\\n        \", HTML.BUTTON({\n        \"class\": \"btn-primary toggle-menu\"\n      }, \"Add To Menu\"), \"\\n      \" ];\n    }), \"\\n    \" ];\n  }), HTML.Raw('\\n    <i class=\"fa fa-pencil\"></i>\\n    <i class=\"fa fa-trash\"></i>\\n  '));\n}));\n","Template.NoweZadanie.events({\n  'click .fa-close' : function(){\n    Session.set('noweZadanie', false);\n  }\n})\n","Template.PojedynczeZadanie.onCreated(function(){\n  var self = this;\n  self.autorun(function(){\n    var id = FlowRouter.getParam('id');\n    self.subscribe('pojedynczeZadanie', id);\n  });\n});\n\nTemplate.PojedynczeZadanie.helpers({\n  zadanie: ()=> {\n    var id = FlowRouter.getParam('id');\n    return Zadania.findOne({_id: id});\n  }\n});\n","Template.Zadania.onCreated(function(){\n  var self = this;\n  self.autorun(function(){\n    self.subscribe('zadania');\n  });\n});\n\nTemplate.Zadania.helpers({\n  zadania: ()=> {\n    return Zadania.find({});\n  }\n});\n\nTemplate.Zadania.events({\n  'click .nowe-zadanie': () => {\n    Session.set('noweZadanie', true);\n  }\n});\n","Template.Zadanie.onCreated(function(){\n  this.editMode = new ReactiveVar(false);\n  // this.editMode = new ReactiveVar();\n  // this.editMode.set(false);\n});\n\nTemplate.Zadanie.helpers({\n  updateZadanieId: function() {\n    return this._id;\n  },\n  editMode: function(){\n    return Template.instance().editMode.get();\n  }\n});\n\nTemplate.Zadanie.events({\n  'click .toggle-menu': function(){\n    Meteor.call('toggleMenuItem', this._id, this.inMenu);\n  },\n  'click .fa-trash' : function(){\n    Meteor.call('deleteZadanie', this._id);\n  },\n  'click .fa-pencil' : function(event, template){\n    template.editMode.set(!template.editMode.get());\n  }\n});\n","\nTemplate.__checkName(\"ListaUwag\");\nTemplate[\"ListaUwag\"] = new Template(\"Template.ListaUwag\", (function() {\n  var view = this;\n  return [ HTML.Raw('<h1 class=\"page-title\">Moja Lista Uwag</h1>\\n  '), HTML.UL({\n    \"class\": \"lista-uwag\"\n  }, \"\\n    \", Blaze.Each(function() {\n    return Spacebars.call(view.lookup(\"listaUwag\"));\n  }, function() {\n    return [ \"\\n      \", Blaze.Each(function() {\n      return Spacebars.call(view.lookup(\"ingredients\"));\n    }, function() {\n      return [ \"\\n        \", HTML.LI(Blaze.View(\"lookup:name\", function() {\n        return Spacebars.mustache(view.lookup(\"name\"));\n      }), \" - \", HTML.SPAN({\n        \"class\": \"amount\"\n      }, Blaze.View(\"lookup:amount\", function() {\n        return Spacebars.mustache(view.lookup(\"amount\"));\n      }))), \"\\n      \" ];\n    }), \"\\n    \" ];\n  }, function() {\n    return [ \"\\n      \", HTML.LI(\"Please add \", HTML.A({\n      href: \"/ksiazka-zadan\"\n    }, \"zadania\"), \" to your menu\"), \"\\n    \" ];\n  }), \"\\n  \") ];\n}));\n","\nTemplate.__checkName(\"ZadaniaNaDzis\");\nTemplate[\"ZadaniaNaDzis\"] = new Template(\"Template.ZadaniaNaDzis\", (function() {\n  var view = this;\n  return [ HTML.Raw('<h1 class=\"page-title\">My Current Menu</h1>\\n  '), Blaze.Each(function() {\n    return Spacebars.call(view.lookup(\"zadania\"));\n  }, function() {\n    return [ \"\\n    \", Spacebars.include(view.lookupTemplate(\"MenuItem\")), \"\\n  \" ];\n  }, function() {\n    return [ \"\\n    \", HTML.H3(\"Please add \", HTML.A({\n      href: \"/ksiazka-zadan\"\n    }, \"zadania\"), \" to your menu\"), \"\\n  \" ];\n  }) ];\n}));\n\nTemplate.__checkName(\"MenuItem\");\nTemplate[\"MenuItem\"] = new Template(\"Template.MenuItem\", (function() {\n  var view = this;\n  return HTML.DIV({\n    \"class\": \"menu-item\"\n  }, \"\\n    \", HTML.H3(Blaze.View(\"lookup:name\", function() {\n    return Spacebars.mustache(view.lookup(\"name\"));\n  })), \"\\n    \", HTML.P(Blaze.View(\"lookup:desc\", function() {\n    return Spacebars.mustache(view.lookup(\"desc\"));\n  })), \"\\n  \");\n}));\n","Template.ListaUwag.onCreated(function(){\n  var self = this;\n  self.autorun(function(){\n    self.subscribe('zadania');\n  });\n});\n\nTemplate.ListaUwag.helpers({\n  listaUwag: ()=> {\n    return Zadania.find({inMenu: true});\n  }\n});\n","Template.Menu.onCreated(function(){\n  var self = this;\n  self.autorun(function(){\n    self.subscribe('zadania');\n  });\n});\n\nTemplate.Menu.helpers({\n  Zadania: ()=> {\n    return Zadania.find({inMenu: true});\n  }\n});\n","import { Template } from 'meteor/templating';\nimport { ReactiveVar } from 'meteor/reactive-var';\n\nimport './main.html';\n","if (Meteor.isClient){\n  Accounts.onLogin(function(){\n    FlowRouter.go('ksiazka-zadan');\n  });\n\n  Accounts.onLogout(function(){\n    FlowRouter.go('home');\n  });\n}\n\nFlowRouter.triggers.enter([function(context, redirect){\n  if(!Meteor.userId()){\n    FlowRouter.go('home');\n  }\n}]);\n\nFlowRouter.route('/', {\n  name: 'home',\n  action(){\n    if(Meteor.userId()){\n      FlowRouter.go('ksiazka-zadan')\n    }\n    BlazeLayout.render('HomeLayout');\n  }\n});\n\nFlowRouter.route('/ksiazka-zadan', {\n  name: 'ksiazka-zadan',\n  action (){\n    BlazeLayout.render('MainLayout', {main: 'Zadania'});\n  }\n});\n\nFlowRouter.route('/zadanie/:id', {\n  name: 'zadanie',\n  action (){\n    BlazeLayout.render('MainLayout', {main: 'PojedynczeZadanie'});\n  }\n});\n\nFlowRouter.route('/zadania-na-dzis', {\n  name: 'zadania-na-dzis',\n  action(){\n    BlazeLayout.render('MainLayout', {main: 'ZadaniaNaDzis'});\n  }\n});\n\nFlowRouter.route('/lista-uwag', {\n  name: 'lista-uwag',\n  action(){\n    BlazeLayout.render('MainLayout', {main: 'ListaUwag'});\n  }\n});\n","Zadania = new Mongo.Collection('zadania');\n\nZadania.allow({\n  insert: function(userId, doc){\n    return !!userId;\n  },\n  update: function(userId, doc){\n    return !!userId;\n  }\n});\n\nIngredient = new SimpleSchema({\n  name: {\n    type: String\n  },\n  amount: {\n    type: String\n  }\n});\n\nZadanieSchema = new SimpleSchema({\n  name: {\n    type: String,\n    label: \"Name\"\n  },\n  desc: {\n    type: String,\n    label: \"Description\"\n  },\n  ingredients: {\n    type: [Ingredient]\n  },\n  inMenu: {\n    type: Boolean,\n    defaultValue: false,\n    optional: true,\n    autoform: {\n      type: \"hidden\"\n    }\n  },\n  author: {\n    type: String,\n    label: \"Author\",\n    autoValue: function(){\n      return this.userId\n    },\n    autoform: {\n      type: \"hidden\"\n    }\n  },\n  createAt: {\n    type: Date,\n    label: \"Created At\",\n    autoValue: function(){\n      return new Date()\n    },\n    autoform: {\n      type: \"hidden\"\n    }\n  }\n});\n\nMeteor.methods({\n  toggleMenuItem: function(id, currentState){\n    Zadania.update(id, {\n      $set: {\n        inMenu: !currentState\n      }\n    });\n  },\n  deleteZadanie: function(id){\n    Zadania.remove(id);\n  }\n});\n\nZadania.attachSchema( ZadanieSchema );\n"]}}]